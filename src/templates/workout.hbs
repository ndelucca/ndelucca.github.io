{{> partials/top_boilerplate}}
  <h1>Workout</h1>
  <div class="workout-table-container">
    <table class="workout-table">
      <thead>
        <tr>
          <th rowspan="2">#</th>
          <th colspan="3">CALENTAMIENTO</th>
          <th colspan="2">E</th>
          <th colspan="2">F</th>
          <th rowspan="2">MAX</th>
        </tr>
        <tr class="percentage-row">
          <th>55%</th>
          <th>65%</th>
          <th>75%</th>
          <th>min<br>80%</th>
          <th>max<br>87.5%</th>
          <th>min<br>90%</th>
          <th>max<br>97.5%</th>
        </tr>
      </thead>
      <tbody id="workout-data">
        <!-- Data will be populated by JavaScript -->
      </tbody>
    </table>
  </div>
{{> partials/bottom_boilerplate}}